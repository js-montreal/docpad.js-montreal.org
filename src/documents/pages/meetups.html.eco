---
title: Meet-Ups
layout: page
pageOrder: 3
---
<% json = @getMeetups('meetups') %>
<ul class="meetup-list">
  <% for meetup in json: %>
    <li>
      <span class="meetup-title"><%= meetup.title %> - <%= meetup.on %></span>
      <p class="description" style="margin-left: 10px"><%= meetup.blurb %></p>
    </li>
  <% end %>
</ul>
